{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "authorizenet/authorizenet": "^2.0",
        "barryvdh/laravel-dompdf": "^0.9.0",
        "czproject/git-php": "^4.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^6.3",
        "kwn/number-to-words": "^1.10",
        "laravel/framework": "^7.0",
        "laravel/passport": "9.x",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "laravolt/avatar": "^4.0",
        "lcobucci/jwt": "3.3.3",
        "maatwebsite/excel": "^3.1",
        "mikehaertl/php-pdftk": "^0.8.1",
        "qcod/laravel-gamify": "^1.0",
        "spatie/laravel-permission": "^3.13",
        "studio/laravel-totem": "^7.0",
        "swisnl/laravel-fulltext": "^0.19.0",
        "unisharp/laravel-filemanager": "^2.1",
        "van-ons/laraberg": "^1.1"
    },
    "require-dev": {
        "beyondcode/laravel-er-diagram-generator": "^1.5",
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "laravel/dusk": "^6.5",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
